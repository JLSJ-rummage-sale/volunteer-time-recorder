<% @page_title = "Edit Event" %>

<h1>Edit Event</h1>

<%= form_for :event, url: event_path(@event), method: :patch do |form| %>

    <%= render(:partial => 'form', :locals => {:form => form}) %>

    <p>
        <%= form.submit("Save Event", {:class => 'btn btn-success btn-block'}) %>
    </p>
<% end %>
