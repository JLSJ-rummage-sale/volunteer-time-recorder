<% @page_title = "Edit Time Record" %>

<h1>Edit Time Record</h1>

<%= form_for :time_record, url: time_record_path(@time_record), method: :patch  do |form| %>

    <%= render(:partial => 'form', :locals => {:form => form}) %>

    <p>
        <%= form.submit("Save Time Record", {:class => 'btn btn-success btn-block'}) %>
    </p>
<% end %>
