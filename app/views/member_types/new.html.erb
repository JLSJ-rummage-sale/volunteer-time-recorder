<% @page_title = "Add Member Type" %>

<h1>Add Member Type</h1>

<%= form_for :member_type, url: member_types_path do |form| %>

    <%= render(:partial => 'form', :locals => {:form => form}) %>

    <p>
        <%= form.submit("Add Member Type", {:class => 'btn btn-success btn-block'}) %>
    </p>
<% end %>
